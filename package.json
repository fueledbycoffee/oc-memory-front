{
  "name": "memory-front",
  "version": "1.0.0",
  "description": "Jeu du \"Memory\" pour O'Clock",
  "scripts": {
    "start": "node index.js",
    "build": "webpack --config config/webpack.dev.config.js --mode=production",
    "dev": "webpack serve --config config/webpack.dev.config.js --mode=development",
    "production": "webpack --config config/webpack.prod.config.js --mode=production",
    "lint:sass": "sass-lint -v -q",
    "lint:js": "eslint --ext .js src/js/",
    "stats": "webpack --config config/webpack.prod.config.js --mode=production --json > dist/stats.json && webpack-bundle-analyzer dist/stats.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fueledbycoffee/oclock-memory-front.git"
  },
  "author": "Sion GENDERS",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fueledbycoffee/oclock-memory-front/issues"
  },
  "homepage": "https://github.com/fueledbycoffee/oclock-memory-front#readme",
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.13.12",
    "autoprefixer": "^10.2.5",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^8.1.0",
    "css-loader": "^5.1.3",
    "css-minimizer-webpack-plugin": "^1.3.0",
    "eslint": "^7.22.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.1",
    "image-minimizer-webpack-plugin": "^2.2.0",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-jpegtran": "^7.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-svgo": "^9.0.0",
    "mini-css-extract-plugin": "^1.3.9",
    "postcss": "^8.2.8",
    "postcss-loader": "^5.2.0",
    "sass": "^1.32.8",
    "sass-lint": "^1.13.1",
    "sass-loader": "^11.0.1",
    "url-loader": "^4.1.1",
    "webpack": "^5.27.2",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  },
  "dependencies": {
    "express": "^4.17.1",
    "svgo": "^2.3.0"
  }
}
